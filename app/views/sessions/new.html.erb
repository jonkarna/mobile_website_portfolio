<h1>Login</h1>
<% form_tag(sessions_url, :method=>'post', :multipart => true) do %>
	<p>
		<%= label_tag 'username' %><br />
		<%= text_field_tag "username", params[:username] %>
	</p>
	<p>
		<%= label_tag 'password' %><br />
		<%= password_field_tag "password" %>
	</p>
	<p>
		<%= submit_tag 'Login' %>
	</p>
<% end %>
<ul class="individual">
	<li><%= link_to 'Back to App', root_path %></li>
</ul>